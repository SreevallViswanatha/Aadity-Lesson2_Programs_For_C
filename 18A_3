#include <stdio.h>
#include <stdlib.h>
//If the cost price and selling price of an item is input through the keyboard, write a program to determine whether the seller has made profit or incurred loss.
int main()
{
    float cost_price, selling_price;
    printf("Enter the cost price:");
    scanf("%f", &cost_price);
    printf("Enter the selling price:");
    scanf("%f", &selling_price);
    cost_price-selling_price>(0)?printf("The seller has incurred loss."):cost_price-selling_price==(0)?printf("The seller has neither made profit or incurred loss."):printf("The seller has made profit.");
}
