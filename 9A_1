#include <stdio.h>
#include <stdlib.h>
//Write a C program to check whether a triangle can be formed by the given value for the angles.
int main()
{
    int a, b, c, m;
    printf("Enter one angle of a triangle:");
    scanf("%d", &a);
    printf("Enter another angle of a triangle:");
    scanf("%d", &b);
    printf("Enter another angle of a triangle:");
    scanf("%d", &c);
    m=a+b+c;
    if(m==180)
    {
        printf("The triangle is valid.");
    }
    else
    {
        printf("The triangle is not valid.");
    }
}
