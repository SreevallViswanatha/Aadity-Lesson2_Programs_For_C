#include <stdio.h>
#include <stdlib.h>
//Any Year is entered through the keyboard, WAP to determine whether the year is  leap or not. Use logical operators.
int main()
{
    int year;
    printf("Enter a year:");
    scanf("%d", &year);
    if((year%400==0)||((year%4==0)&&(year%100!=0)))
    {
        printf("It is a leap year.");
    }
    else
    {
        printf("It is not a leap year.");
    }
}
