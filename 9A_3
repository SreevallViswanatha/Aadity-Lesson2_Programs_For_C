#include <stdio.h>
#include <stdlib.h>
//Write a C program to check whether an alphabet is a vowel or consonant.
int main()
{
    char b;
    printf("Enter a letter:");
    scanf("%c", &b);
    if((b==65)||(b==69)||(b==73)||(b==79)||(b==85)||(b==97)||(b==101)||(b==105)||(b==111)||(b==117))
    {
        printf("The letter is a vowel.");
    }
    else
    {
        printf("The letter is not a vowel.");
    }
}
