#include <stdio.h>
#include <stdlib.h>
//WAP to ask user to enter a number less than 10, if the user enters a number less than 10, print : “What an obedient servant you are!”
int main()
{
    int a;
    printf("Enter a number from 1 to 9:");
    scanf("%d", &a);
    if(a<10)
    {
        printf("What an obedient servant you are!");
    }
}
