#include <stdio.h>
#include <stdlib.h>
//Using conditional operators determine:
//a.	Whether the character entered through the keyboard is a lower case alphabet or not.
//b.	Whether a character entered through the keyboard is a special symbol or not.
int main()
{
    char character;
    printf("Enter a character:");
    scanf("%c", &character);
    if((character>=65)&&(character<=90))
    {
        printf("It is a uppercase letter.");
    }
    else if((character>=97)&&(character<=122))
    {
        printf("It is a lowercase letter.");
    }
    else
    {
        printf("It is a special symbol.");
    }
}
