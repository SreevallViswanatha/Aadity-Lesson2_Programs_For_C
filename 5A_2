#include <stdio.h>
#include <stdlib.h>
//WAP to accept marks in 2 subjects and display the student is pass if he gets more than 50 in each subject (or) more than 120 in both the subjects (or) more than 80 in any 1 subject.
int main()
{
    int a, b;
    printf("Enter the marks in a subject:");
    scanf("%d", &a);
    printf("Enter the marks in a subject:");
    scanf("%d", &b);
    if(((a>50)&&(b>50))||(a+b>120)||((a>80)||(b>80)))
    {
        printf("The student has passed.");
    }
    else
    {
        printf("The student has failed.");
    }
}
